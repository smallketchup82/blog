---
import { ClientRouter } from "astro:transitions";
const { title = "smallketchup82" } = Astro.props;
const { description } = Astro.props;
---

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="icon" type="image/x-icon" href="/favicon.ico" />
  <link rel="sitemap" href="/sitemap-index.xml" />
  <meta name="generator" content={Astro.generator} />
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=Megrim&family=Oxygen&family=Quicksand&family=Oxygen+Mono&display=swap"
    rel="stylesheet"
  />
  <title>{title}</title>
  <meta property="og:title" content="smallketchup82 blog" />
  <meta property="og:description" content="terrible sense of humor or sum" />
  <ClientRouter />
</head>
